{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", 
    "**/*.ts", 
    "**/*.js", 
    "**/*.tsx", 
    "**/*.jsx", 
    ".next/types/**/*.ts", 
    "lib/db/drizzle.js", 
    "app/workspace/(meta)/dwh-config/page.jsx", 
    "components/master-details.tsx", 
    "components/details.js", 
    "lib/ErrorBoundary.jsx", 
    "pages/api/processEntityItem.js", 
    "pages/api/processEntityItem.js", 
    "app/workspace/layout.jsx"
, "app/workspace/layout copy.__tsx"  ],
  "exclude": ["node_modules"]
}
